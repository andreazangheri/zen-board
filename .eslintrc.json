{
  "extends": ["eslint:recommended", "prettier", "prettier/react", "plugin:compat/recommended"],
  "plugins": ["compat"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "compat/compat": "error"
  }
}
